    	"lhc_telegram_bot" : "CREATE TABLE `lhc_telegram_bot` (  `id` bigint(20) unsigned NOT NULL AUTO_INCREMENT, `bot_username` varchar(50) NOT NULL, `delete_on_close` tinyint(1) unsigned NOT NULL DEFAULT '0', `bot_disabled` tinyint(1) unsigned NOT NULL, `bot_api` varchar(50) NOT NULL, `group_chat_id` bigint(20) NOT NULL, `dep_id` bigint(20) unsigned NOT NULL, `bot_client` bigint(20) unsigned NOT NULL, `webhook_set`  bigint(20) unsigned NOT NULL, `chat_timeout`  bigint(20) unsigned NOT NULL, PRIMARY KEY (`id`)) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4;",
