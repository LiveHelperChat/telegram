    	"lhc_telegram_operator" : "CREATE TABLE `lhc_telegram_operator` (`id` bigint(20) unsigned NOT NULL AUTO_INCREMENT, `bot_id`  bigint(20) unsigned NOT NULL, `chat_id` bigint(20) unsigned NOT NULL,`tchat_id` bigint(20) unsigned NOT NULL, `tuser_id` bigint(20) unsigned NOT NULL, `user_id` bigint(20) unsigned NOT NULL ,`confirmed` tinyint(1) unsigned NOT NULL, PRIMARY KEY (`id`), KEY `bot_id` (`bot_id`)) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4;",
